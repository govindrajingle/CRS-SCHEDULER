}
.top-header .container-fluid {
    padding: 0 30px;
}
.logo-col ul li {
    line-height: normal;
}

header .navigation a {
    text-decoration: none;
}

.logo-col ul .logo-name {
    color: var(--darkBlue);
    text-transform: uppercase;
    font-weight: 700;
    font-size: 21px;
    font-family: var(--font-roboto-condensed);
    line-height: 1;
}
.logo-col ul .mci {
    color: #000;
    font-family: var(--font-poppins);
    font-weight: 500;
    font-size: 16px;
    line-height: 1;
    margin:5px 0 4px;
}

.logo-col ul .goi {
    color: var(--black);
    font-weight: 500;
    line-height: 20px;
    font-size: 14px;
    line-height: 1;
}
nav .navigation {
    justify-content: flex-end;
}

.navigation .nav-links {
    display: inline-flex;
}

.nav-links li {
    margin: 1px 3px;
}
.nav-links>li>a {
    color: var(--black);
    text-decoration: none;
    font-size: 16px;
    font-weight: 600;
    padding: 9px 8px;
    border-radius: 5px;
    position: relative;
    width: 100%;
    transition: linear .5s;
    -webkit-transition: linear .5s;
    -moz-transition: linear .5s;
    -o-transition: linear .5s;
}
.nav-links>li a:before {
    content: "";
    position: absolute;
    left: 0;
    bottom: 0;
    height: 0;
    width: 100%;
    background-color: var(--orange);
    transition: linear .4s;
    border-radius: 20px;
    margin: 0 auto;
    right: 0;
    max-width: 70%;
}

.nav-links>li:hover>a, .nav-links>li.current-menu-item>a {
    background-color: var(--orange);
    color: var(--darkblue);
    border-radius: 8px;
}
.nav-links>li:hover>a:before {
    bottom: calc(100% - 3px);
    height: 1px;
}

.nav-links>li.current-menu-item>a:before {
    height: 1px;
    width: 100%;
    top: 0;
}

.nav-links .mobile-item {
    display: none;
}

.nav-links .drop-menu {
    position: absolute;
    background: var(--white);
    width: 210px;
    line-height: 45px;
    top: 27px;
    opacity: 0;
    visibility: hidden;
    box-shadow: 0 6px 10px rgba(0, 0, 0, 0.15);
}
.nav-links li:hover .drop-menu {
    transition: all 0.3s ease;
    top: 27px;
    opacity: 1;
    visibility: visible;
    z-index: 12;
    background:var(--black);
    border-radius: 4px;
}
.drop-menu li a {
    width: 100%;
    display: block;
    padding: 4px 5px 4px;
    font-weight: 500;
    border-radius: 0px;
    color: var(--white);
    line-height: normal;
    border-bottom: 1px solid var(--white);
}
.drop-menu li a:hover {
    background: var(--white);
    color: var(--black);
}
.navigation .btn {
    color: var(--black);
    font-size: 30px;
    cursor: pointer;
    display: none;
}

.navigation .btn.close-btn {
    position: absolute;
    right: 30px;
    top: 10px;
}
nav input, .logo-img-mob {
    display: none;
}


