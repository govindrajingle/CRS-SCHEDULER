/****************************
media Query
****************************/
@media (max-width: 1199px) {
.pagination li a {
    min-width: 35px;
    height: 35px;
    padding: 5px;
}
}

@media (max-width: 991px) {
.nav-sidebar> .nav-item>.nav-link {
    display: flex;
}
.changePassword_page .panel-body{
    width: 100%;
}
}

@media (max-width: 767px) {
body .content.content-tiles {
    padding: 16px 12px;
}
    .layout-navbar-fixed .wrapper .main-header {
    padding: 6px 20px;
}
.more_info a {
    width: 100%;
    text-align: center;
    margin: 10px 0 0;
    font-size: 14px;
}
.content-header .breadcrumb {
    float: none !important;
    width: 100%;
}
body .cdlogo p{
        text-align: center;
    margin: 0;
}
.img_footer {
    text-align: center !important;
}
body .card-section {
    padding: 0 10px 10px;
}
body .phadmin .card {
    min-height: auto;
}
.modal-footer {
    flex-wrap: wrap;
    justify-content: center;
}
body div.dataTables_wrapper div.dataTables_paginate ul.pagination{
    justify-content: center;
    margin-top: 10px;
}
}

@media (max-width: 580px) {
.pagination li a {
    min-width: 30px;
    height: 30px;
    padding: 5px;
}
div.dataTables_wrapper div.dataTables_paginate ul.pagination {
    justify-content: center;
}
}

@media (max-width: 575px) {
form#frm .col {
    flex: none;
}
.main-header .navbar-nav[class*='-right'] .dropdown-menu {
    left: auto;
    right: 0;
}
}

@media (max-width: 480px) {
body .custom-btn, body .btn_default, body div.dataTables_wrapper .dataTables_filter label {
    width: 100%;
}
body div.dataTables_wrapper .dataTables_filter input[type="search"] {
    width: 100%;
    display: block;
    margin: 0;
    box-sizing: border-box;
}
}
