.container-fluid {
    width: 100%;
    padding: 0 20px;
    max-width: 100%;
}
/**Header**/
/**********top header*************/
.top-header {
    background: var(--lightGray);
    padding: 2px 0 0px;
    overflow: hidden;
    background-image: linear-gradient(to bottom, #202852, #0a2f59);
    border-top: 1px solid rgb(0 0 0);
    clear: both;
}
.digi_logos a img {
    height: 38px;
    object-fit: contain;
    max-width: 100px;
}
/*.top-header ul.topbar_menu{
    justify-content: flex-end;
}*/
.top-header ul li {
    padding: 0 10px;
}
ul.d-flex.digi_logos {
    justify-content: flex-end;
}
.top-header ul.topbar_menu li:after {
    position: absolute;
    content: '';
    background: #c7d3e3;
    height: 50px;
    width: 1px;
    right: 0;
    /* transform: skew(20deg); */
    top: -10px;
}
.top-header ul.topbar_menu li:last-child:after{
    display: none;
}
.top-header ul.topbar_menu li.menu-item a {
    color: var(--white);
    font-size: 14px;
    font-weight: 500;
}
/*Dark theme*/
header .header_toggle .checkbox {
  opacity: 0;
  position: absolute;
}

header .header_toggle .checkbox-label {
    background-color: #111;
    width:40%;
    min-height: 30px;
    border-radius: 50px;
    position: relative;
    padding: 5px;
    cursor: pointer;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 0 auto 7px;
    font-size: 22px;
}
header .header_toggle .fa-moon {color: #f1c40f;}

header .header_toggle .fa-sun {color: #f39c12;}

header .header_toggle .checkbox-label .ball {
  background-color: var(--white);
  width: 26px;
  height: 26px;
  position: absolute;
  left: 2px;
  top: 2px;
  border-radius: 50%;
  transition: transform 0.2s linear;
}

header .header_toggle .checkbox:checked + .checkbox-label .ball {
  transform: translateX(41px);
}
/*end Dark theme*/
.font-increse #chcp_font_size span {
    width: auto;
}
#chcp_font_size button:hover, .font-increse a:hover {
    background: var(--darkBlue);
    color: var(--white);
}
#chcp_font_size button, .font-increse a {
    margin: 0 auto 6px;
    font-weight: 600;
    padding: 7px;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    align-items: center;
    transition: linear .5s;
    -webkit-transition: linear .5s;
    -moz-transition: linear .5s;
    -o-transition: linear .5s;
    justify-content: center;
    font-size: 14px;
    background: #fff;
    color: #000;
    border: 1px solid var(--black);
    border-radius: 10px;
    width: 100%;
}
.sidepanel a i {
    padding-right: 5px;
}
li.font-increse ul li, li.font-increse {
    padding: 0;
}
.menu-item-mob, .mob-toggle .login-btn{
    display: none;
}
.top-header li.header_toggle, .top-header li.font-increse {
    border: 1px solid #000;
    padding: 10px 20px;
    width: 90%;
    margin: 0 auto 10px;
    border-radius: 10px;
    background: #f3f2f2;
}
.top-header li.header_toggle h5, .top-header li.font-increse h5 {
    margin-bottom: 10px;
    font-size: 20px;
}
/***********accessible side toogle************/
/* The sidepanel menu */
.sidepanel {
    min-height: 250px;
    width: 0;
    position: fixed;
    z-index: 111111;
    top: 0;
    right: 0;
    background-color: #ddd;
    overflow-x: hidden;
    padding-top: 60px;
    transition: 0.5s;
    border-radius: 0px 0px 10px 10px;
    box-shadow: 3px 1px 5px #a5a5a5;
    border: 1px solid #ddd;
}
/* The sidepanel links */
.sidepanel a {
    text-decoration: none;
    display: block;
    transition: 0.3s;
}


/* Position and style the close button (top right corner) */
.sidepanel .closebtn {
    position: absolute;
    top: 10px;
    right: 25px;
    font-size: 26px;
    margin-left: 50px;
    background: var(--darkBlue);
    color: #fff;
    width: 25px;
    height: 25px;
    text-align: center;
    border-radius: 100px;
    line-height: 25px;
}

/* Style the button that is used to open the sidepanel */
.openbtn {
  font-size: 20px;
  cursor: pointer;
  background-color: #111;
  color: white;
  padding: 10px 15px;
  border: none;
}

.openbtn:hover {
  background-color: #444;
}
.dark #mySidepanel {
    background: #000;
}
.dark .top-header li.header_toggle, .dark .top-header li.font-increse {
    background: #323232;
}
.dark .sidepanel .closebtn {
    color: var(--white);
}
.dark .top-header li.header_toggle h5, .dark .top-header li.font-increse h5 {
    color: #fff;
}
/***********main-header************/
.main-header {
    background: var(--white);
