/****************************
pagination
****************************/
.pagination li {
    margin: 0 2px 3px;
}
.pagination li a {
    background: transparent;
    border: 1px solid var(--darkBlue);
    color: var(--darkBlue);
    font-weight: 500;
    font-family: var(--font-poppins);
    min-width: 40px;
    height: 40px;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 8px;
        border-radius: 10px;
}
.pagination li a:hover, .pagination li.page-item.active a {
    background: var(--green);
    border: 1px solid var(--green);
    color: var(--darkBlue);
}
