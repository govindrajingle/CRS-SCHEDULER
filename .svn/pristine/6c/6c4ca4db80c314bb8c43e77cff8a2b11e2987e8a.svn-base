}
.section-title h2 {
    text-transform: inherit;
    font-size: 28px;
    font-weight: 700;
    position: relative;
    color: var(--black);
    line-height: normal;
    padding-bottom: 15px;
    margin-bottom: 15px;
}
.section-title h2:after {
    position: absolute;
    content: '';
    background: var(--green);
    width: 100%;
    max-width: 40px;
    height: 7px;
    bottom: 0;
    left: 0;
}
