<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="MANUFACTURER_LIST">
    <xs:complexType>
      <xs:sequence>
			  <xs:element name="FILENAME">
		          <xs:simpleType>
		            <xs:restriction base="xs:string">
		              <xs:length value="24"/>
		              <xs:pattern value="[0-9]{10}MAN[0-9]{11}"/>
		            </xs:restriction>
		          </xs:simpleType>
		      </xs:element>
              <xs:element name="MANF_CODE">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="7"/>
                    <xs:maxLength value="10"/>
                    <xs:pattern value="([0-9])*"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element maxOccurs="unbounded" name="MANUFACTURER">
                <xs:complexType>
                  	<xs:sequence>
                       <xs:element name="EXPO_TYPE">
	                    <xs:simpleType>
	                      <xs:restriction base="xs:string">
	                        <xs:length value="4"/>
	                        <xs:pattern value="MANF|MRCH"/>
	                      </xs:restriction>
	                    </xs:simpleType>
	                  </xs:element>
                      <xs:element name="MANF_NAME">
                        <xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:minLength value="2"/>
                            <xs:maxLength value="50"/>
                            <xs:pattern value="([a-zA-Z0-9/&amp;\.\-_`\s%\(\)!\*+,:;=&lt;&gt;])*"/>
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:element>
                      <xs:element name="MANF_STAT">
                        <xs:simpleType>
                          <xs:restriction base="xs:string">
                             <xs:length value="1"/>
	                         <xs:pattern value="A|I"/>
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:element>
                      <xs:element name="MANF_PANN">
                        <xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:length value="10"/>
                            <xs:pattern value="[A-Z]{5}[0-9]{4}[A-Z]{1}"/>
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:element>
                   <xs:element name="MANF_LICN">
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                       <xs:minLength value="5"/>
                       <xs:maxLength value="50"/>
                       <xs:pattern value="([a-zA-Z0-9/&amp;\.\-_`\s%\(\)!\*+,:;=&lt;&gt;])*"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
	              <xs:element name="MANF_IECN">
	                <xs:simpleType>
	                  <xs:restriction base="xs:string">
	                    <xs:length value="10"/>
	                    <xs:pattern value="([a-zA-Z0-9/&amp;\.\-_`\s%\(\)!\*+,:;=&lt;&gt;])*"/>
	                  </xs:restriction>
	                </xs:simpleType>
	              </xs:element>
	              <xs:element name="MANF_TINN">
	                <xs:simpleType>
	                  <xs:restriction base="xs:string">
	                    <xs:length value="11"/>
                        <xs:pattern value="[0-9]{11}"/>
	                  </xs:restriction>
	                </xs:simpleType>
	              </xs:element>
 			  		<xs:element name="MANF_ADDR">
                    	<xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:minLength value="10"/>
                       		<xs:maxLength value="100"/>
                       		<xs:pattern value="([a-zA-Z0-9/&amp;\.\-_`\s%\(\)!\*+,:;=&lt;&gt;])*"/>
                          </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="MANF_RAJY">
                    	<xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:length value="2"/>
                        	<xs:pattern value="[0-9]{2}"/>
                          </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="MANF_DIST">
                    	<xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:length value="3"/>
                        	<xs:pattern value="[0-9]{3}"/>
                          </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="MANF_CITY">
                    	<xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:minLength value="2"/>
                       		<xs:maxLength value="50"/>
                       		<xs:pattern value="([a-zA-Z\s])*"/>
                          </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="MANF_PINC">
                    	<xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:length value="6"/>
                        	<xs:pattern value="[0-9]{6}"/>
                          </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="MANF_LAND" minOccurs="0">
                    	<xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:minLength value="10"/>
                       		<xs:maxLength value="15"/>
                        	<xs:pattern value="[0-9]"/>
                          </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="MANF_EMAL">
                    	<xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:minLength value="5"/>
                       		<xs:maxLength value="75"/>
                        	<xs:pattern value="(\w[-._\w]*\w@\w[-._\w]*\w\.\w{2,4})*"/>
                          </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="MANF_WEBS">
                    	<xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:minLength value="5"/>
                       		<xs:maxLength value="75"/>
                        	<xs:pattern value="([-\w.]+(:\d{2,5})?(/([\w/_.]*)?)?)*"/>
                          </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="MANF_CPER">
                    	<xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:minLength value="2"/>
                       		<xs:maxLength value="50"/>
                        	<xs:pattern value="([a-zA-Z\s])*"/>
                          </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="MANF_CDSG">
                    	<xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:minLength value="2"/>
                       		<xs:maxLength value="50"/>
                        	<xs:pattern value="([a-zA-Z0-9/&amp;\.\-_`\s%\(\)!\*+,:;=&lt;&gt;])*"/>
                          </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="MANF_CMOB">
                    	<xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:length value="10"/>
                        	<xs:pattern value="[0-9]{10}"/>
                          </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="MANF_CEML">
                    	<xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:minLength value="5"/>
                       		<xs:maxLength value="75"/>
                        	<xs:pattern value="(\w[-._\w]*\w@\w[-._\w]*\w\.\w{2,4})*"/>
                          </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="GSTIN_NO" minOccurs="0">
                    	<xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:minLength value="1"/>
                       		<xs:maxLength value="15"/>
                        	<xs:pattern value="([a-zA-Z0-9/&amp;\.\-_`\s%\(\)!\*+,:;=&lt;&gt;])*"/>
                          </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="GS1_NO" minOccurs="0">
                    	<xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:minLength value="1"/>
                       		<xs:maxLength value="15"/>
                        	<xs:pattern value="([a-zA-Z0-9/&amp;\.\-_`\s%\(\)!\*+,:;=&lt;&gt;])*"/>
                          </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                 </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
  </xs:element>
</xs:schema>