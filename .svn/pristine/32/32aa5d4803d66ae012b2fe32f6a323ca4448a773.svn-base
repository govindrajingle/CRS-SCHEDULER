/****************************
Video Tutorial
****************************/
.reg_note {
    background: #ededed;
    padding: 40px 0;
}
.reg_colvideo h3 {
    color: var(--black);
    font-weight: bold;
    font-size: 18px;
    letter-spacing: 0;
    line-height: normal;
    margin: 30px 0 0px;
}
.reg_colvideo .col {
    margin-bottom: 20px;
}
.reg_note .card {
    background: var(--white);
    border-radius: 10px;
    overflow: hidden;
    flex-direction: column;
    justify-content: center;
    box-shadow: 0 14px 26px rgba(0, 0, 0, 0.04);
    transition: all 0.3s ease-out;
    -moz-transition: all 0.3s ease-out;
    -webkit-transition: all 0.3s ease-out;
    text-decoration: none;
    cursor: pointer;
    padding: 20px 20px;
    margin: 0;
    border: 1px solid #ddd;
}
.reg_note .card:hover {
  transform: translateY(-5px) scale(1.005) translateZ(0);
  -webkit-transform: translateY(-5px) scale(1.005) translateZ(0);
  -moz-transform: translateY(-5px) scale(1.005) translateZ(0);
  box-shadow: 0 24px 36px rgba(0, 0, 0, 0.11),
    0 24px 46px var(--box-shadow-color);
}

.reg_note .card:hover .overlay {
    transform: scale(4) translateZ(0);
    opacity: 1;
}

.reg_note .card:hover .circle {
    border-color: #66c67a4f;
    background: transparent;
}

.reg_note .card:hover .circle:after {
  background: var(--white);
}

.reg_note .circle {
    width: 110px;
    height: 110px;
    border-radius: 50%;
    background:var(--white);
    border: 2px solid #66c67a8f;
    display: -webkit-box;   /* OLD - iOS 6-, Safari 3.1-6, BB7 */
display: -ms-flexbox;  /* TWEENER - IE 10 */
display: -webkit-flex; /* NEW - Safari 6.1+. iOS 7.1+, BB10 */
display: flex;    
    justify-content: center;
    align-items: center;
    position: relative;
    z-index: 1;
    transition: all 0.3s ease-out;
    -webkit-transition: all 0.3s ease-out;
    -moz-transition: all 0.3s ease-out;
}
.reg_note .circle:after {
    content: "";
    width: 90px;
    height: 90px;
    display: block;
    position: absolute;
    background: #66c67a4f;
    border-radius: 50%;
    top: 8px;
    left: 0;
    transition: opacity 0.3s ease-out;
    -webkit-transition: opacity 0.3s ease-out;
    -moz-transition: opacity 0.3s ease-out;
    right: 0;
    margin: 0 auto;
}

.reg_note .circle i {
  z-index: 10000;
  transform: translateZ(0);
  -webkit-transform: translateZ(0);
  -moz-transform: translateZ(0);
}

.reg_note .overlay {
    width: 100%;
    position: absolute;
    height: 100%;
    border-radius: 50%;
    background: #66c67a21;
    top: 0px;
    left: 0px;
    z-index: 0;
    transition: transform 0.3s ease-out;
    -webkit-transition: transform 0.3s ease-out;
    -moz-transition: transform 0.3s ease-out;
    opacity: 0;
}
.reg_note .col2 .card:hover .overlay {
    background:#fc933530 ;
}
.reg_note .col2 .card:hover .circle {
    border:  2px solid var(--orange);
}
.reg_note .col1 .card:hover .circle i {
    color: #66c67a;
}
.reg_note .col2 .card:hover .circle i {
    color: var(--orange);
}
.reg_note .col3 .card:hover .overlay {
    background:  #5ec9ea21;
}
.reg_note .col3 .card:hover .circle {
    border:2px solid #5ec9ea73;
}
.reg_note .col3 .card:hover .circle i {
    color: #5ec9ea;
}
.reg_note .col4 .card:hover .overlay {
    background: #67666629;
}
.reg_note .col4 .card:hover .circle {
    border: 2px solid var(--gray);
}
.reg_note .col4 .card:hover .circle i {
    color: var(--gray);
}
.reg_note .col5 .card:hover .overlay {
    background: #062c5a21;
}
.reg_note .col5 .card:hover .circle {
    border: 2px solid var(--darkBlue);
}
.reg_note .col6 .card:hover .overlay {
    background: #ffa7a73b;
}
