Dashboard page1
****************************/
body .bg_white{
        background-color: #fff;
    border: 1px solid transparent;
        margin-bottom: 20px;
    border-radius: 30px;
    box-shadow: -2px 0px 10px 0px #bbb;
        border-radius: 10px;
    padding: 15px 10px;
}
.page-title {
    background: #dfdede;
    padding: 5px 10px;
    border-radius: 10px;
    margin-bottom: 20px;
    border: 1px solid  #d5d2d2;
}
.page-title h1 {
    font-size: 32px;
    text-transform: capitalize;
    line-height: normal;
}

.card-section .card-footer .numbers a {
    color: #fff;
}
.card-section .card-stats .card-body .numbers .card-category {
    color: #fff;
    line-height: 1.2;
    font-size: 16px;
    font-weight: 500;
    margin-top: 10px;
    display: inline-block;
    text-shadow: 5px 2px 11px #000;
}
.bg-icon {
    position: absolute;
    top: 2%;
    left: 0;
    opacity: 0.1;
    filter: brightness(0) invert(1);
    -webkit-filter: brightness(0) invert(1);
    -moz-filter: brightness(0) invert(1);
    -o-filter: brightness(0) invert(1);
    object-fit: contain;
    height: 90px;
    right: 0;
    margin: 0 auto;
}
.card-section .card-content {
    min-height: 160px;
    border-radius:10px;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-wrap: wrap;
    padding: 10px;
}
.card-section .card {
    background: transparent;
    box-shadow: none;
}
.card-section .card-content:hover {
    box-shadow: 0px 7px 10px 0px #7c7a7a;
    border-radius: 20px;
}
.card-section .card-footer {
    border-top: 1px solid #062c5a00;
    width: 100%;
    min-height: auto;
    padding: 0;
}
div#countrywise {
    margin-top: 10px;
    overflow-y: auto;
    overflow-x: hidden;
}
