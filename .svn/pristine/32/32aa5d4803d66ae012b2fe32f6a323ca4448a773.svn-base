.content7.p-0 {
    width: 40%;
    margin: 0 auto;
    display: -webkit-box;   /* OLD - iOS 6-, Safari 3.1-6, BB7 */
	display: -ms-flexbox;  /* TWEENER - IE 10 */
	display: -webkit-flex; /* NEW - Safari 6.1+. iOS 7.1+, BB10 */
	display: flex;    
    height: 100%;
    justify-content: center;
    align-items: center;
    padding: 50px 0;
}
.hero-banner-form {
    position: absolute;
    top: 6%;
    z-index: 11;
    left: 2%;
}
.hero-banner-form button.qrcode-btn {
    background: #46bb5e;
    border: 0;
    font-size: 14px;
    font-weight: 600;
    padding: 3px 8px 3px 4px;
    letter-spacing: -.5px;
    box-shadow: 0 15px 10px -10px rgba(31, 31, 31, 0.5);
    border-radius: 100px;
    position: relative;
    transition: all 0.5s ease;
    -webkit-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
}
.hero-banner-form button.qrcode-btn:hover {
    background: var(--orange);
}
.hero-banner-form button.qrcode-btn:hover:before{
    display: none;
}
.hero-banner-form button.qrcode-btn span {
    font-size: 14px;
    padding: 0 10px;
    transition: all 0.5s ease;
    -webkit-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    color: #000e22;
}
.hero-banner-form button.qrcode-btn i {
    background: #072d55;
    width: 50px;
    height: 50px;
    display: flex;
    text-align: center;
    line-height: 50px;
    border-radius: 100%;
    font-size: 30px;
    color: #fff;
    justify-content: center;
    align-items: center;
}
@keyframes pulse-border {
  0% {
    transform: translateX(-50%) translateY(-50%) translateZ(0) scale(1);
    opacity: 1;
  }
  100% {
    transform: translateX(-50%) translateY(-50%) translateZ(0) scale(1.5);
    opacity: 0;
  }
}
.hero-banner-form button.qrcode-btn:before {
    content: "";
    position: absolute;
    z-index: 0;
    left: 50%;
    top: 50%;
    transform: translateX(-50%) translateY(-50%);
    display: block;
    min-width: 160px;
    height: 48px;
    background: #002c53;
    border-radius: 100px;
    -webkit-animation: pulse-border 1500ms ease-out infinite;
    animation: pulse-border 1500ms ease-out infinite;
    z-index: -1;
}
button.qrcode-btn:after {
    content: '';
    background: linear-gradient(45deg, #072e56, #0b2c55, #fc9335, #3bb755, #61b13b, #00a9de, #00a9de, #00a9de, #666667);
    position: absolute;
    top: -2px;
    left: -2px;
    background-size: 400%;
    z-index: -1;
    filter: blur(5px);
    width: calc(100% + 4px);
    height: calc(100% + 4px);
    animation: glowing 20s linear infinite;
    opacity: 0;
    transition: opacity .3s ease-in-out;
    border-radius: 100px;
}
button.qrcode-btn:hover:after {
    opacity: 1;
}
@keyframes glowing {
    0% { background-position: 0 0; }
    50% { background-position: 400% 0; }
    100% { background-position: 0 0; }
}
.sidenav .closebtn {
    position: absolute;
    top: -10px;
    right: -10px;
    font-size: 30px;
    z-index: 1;
    background: var(--darkBlue);
    width: 25px;
    height: 26px;
    border-radius: 50px;
    color: var(--white);
    display: -webkit-box;   /* OLD - iOS 6-, Safari 3.1-6, BB7 */
	display: -ms-flexbox;  /* TWEENER - IE 10 */
	display: -webkit-flex; /* NEW - Safari 6.1+. iOS 7.1+, BB10 */
	display: flex;    
    justify-content: center;
    align-items: center;
}

