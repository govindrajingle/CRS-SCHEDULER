/* ==============     tooltip in track and trace ===== */


.trck .tooltip {
    display: inline-block;
    position: relative;
    z-index: 1;
    text-align: left;
    opacity: 1 !important;
    float: right;
    font-family: 'Open Sans', sans-serif;
    font-weight: bold;
    font-size: 14px;
}

.tooltip i {
    font-size: 20px;
    text-shadow: none;
}

.tooltip h3 {
    margin: 12px 0;
}

.tooltip .top {
    min-width: 200px;
    max-width: 400px;
    top: -6px;
    right: -409%;
    transform: translate(-30%, -100%);
    padding: 1px 7px;
    color: var(--darkBlue);
    background-color: #f59220;
    font-weight: normal;
    font-size: 14px;
    border-radius: 8px;
    position: absolute;
    z-index: 99999999;
    box-sizing: border-box;
    box-shadow: 0 1px 8px rgba(0, 0, 0, 0.5);
    display: none;
    text-shadow: none;
}

.tooltip:hover .top {
    display: block;
}

.tooltip .top i {
    position: absolute;
    top: 98%;
    width: 30px;
    height: 15px;
    overflow: hidden;
    right: 4%;
}

.tooltip .top i::after {
    content: '';
    position: absolute;
    width: 15px;
    height: 15px;
    left: 50%;
    transform: translate(-50%, -50%) rotate(45deg);
    background-color: #f59220;
    box-shadow: 0 1px 8px rgba(0, 0, 0, 0.5);
}


