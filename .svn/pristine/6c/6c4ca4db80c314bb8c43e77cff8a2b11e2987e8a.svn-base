/********Kamana CSS Code********
 * 06 June 2023********/

 body {
    background: #e5e5e5;
}
.close-toggle {
    display: none;
}
i.fa {
    font-family: 'Font Awesome 5 Free' !important;
    font-weight: 900;
}
i.fa.fa-product-hunt {
    font-family: 'Font Awesome 5 Brands' !important;
}
.icon-big:hover a {
    color: #fff;
}
body div#m_table_1_wrapper, .container-fluid, .m-portlet {
    clear: both;
}

/****************************
Header Main
****************************/
.layout-navbar-fixed .wrapper .main-header {
    box-shadow: 2px 2px 5px #d1d1d1;
    border: 0px;
    background: linear-gradient(to right, rgb(42 103 161), rgb(6 45 86));
}

.main-header .navbar-nav >li> a {
    color: var(--white) !important;
    font-size: 24px;
}
.fa {
    font-family: 'Font Awesome 5 Free';
}
.main-header .nav-right> .nav-item >a {
    width: 35px;
    height: 35px;
    background: var(--orange);
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 0;
    border-radius: 10px;
    font-size: 17px;
}
.main-header .nav-right> .nav-item >a:hover {
    background: var(--green);
}
.main-header .nav-right> .nav-item >a:hover i {
    -moz-transform: rotate(360deg);
    -webkit-transform: rotate(360deg);
    -ms--transform: rotate(360deg);
    transform: rotate(360deg);
    -webkit-transition: all 0.2s;
    -moz-transition: all 0.2s;
    -o-transition: all 0.2s;
    -ms-transition: all 0.2s;
    transition: all 0.2s;
}
.main-header .navbar-nav> .nav-item {
    margin: 0 2px;
}
.main-header .dropdown-menu-header .dropdown-menu-header-inner {
    margin: 0px 0px 0;
    padding: 10px;
    position: relative;
    background-image: linear-gradient(to right, #07266e, #124c68);
}
.main-header .dropdown-menu .dropdown-item {
    border-bottom: 1px solid #ddd;
    color: var(--black);
    font-weight: 500;
    padding: 6px 10px;
}
.main-header .dropdown-menu .dropdown-item:hover {
    background: var(--orange);
    color: var(--darkBlue);
}
.main-header .dropdown-menu-header .dropdown-menu-header-inner h4 {
    font-size: 20px;
}
/****************************
Sidebar
****************************/
.layout-navbar-fixed.layout-fixed .wrapper .sidebar, body.sidebar-collapse.layout-fixed .wrapper .dsh_main aside:hover .sidebar{
    margin-top: calc(12rem + 1px);
    height: calc(100vh - (12rem + 1px));
}
.nav-sidebar> .nav-item>.nav-link .right {
    right: 10px;
    left: auto;
    transform: rotate(-85deg);
    top: 35%;
}
.nav-sidebar .menu-open>.nav-link i.right {
    -webkit-transform: rotate(90deg);
    transform: rotate(90deg);
}
.layout-fixed .main-sidebar {
    background-image: linear-gradient(to bottom, #202852, #053c6d);
}
body.layout-fixed .wrapper .dsh_main aside .brand-link {
    background: #192851 !important;
}
body.layout-fixed .wrapper .dsh_main aside .brand-link img {
    max-height: inherit;
    width: 100%;
    height: 60px;
    margin: 0 auto;
    object-fit: contain;
}
body.layout-fixed .wrapper .dsh_main aside .brand-link {
    background: #192851 !important;
    display: -webkit-box;   /* OLD - iOS 6-, Safari 3.1-6, BB7 */
display: -ms-flexbox;  /* TWEENER - IE 10 */
display: -webkit-flex; /* NEW - Safari 6.1+. iOS 7.1+, BB10 */
display: flex;    
flex-wrap: wrap;
justify-content: center;
box-shadow: -4px 1px 10px -2px #101b38;
border-bottom: 2px solid #31416a;
}
body.layout-fixed .wrapper .dsh_main aside .brand-link span {
    font-size: 16px;
    line-height: 1.3;
    margin: 4px 0 0;
    text-align: center;
    color: #fff;
    width: 100%;
    font-weight: 500;
    font-family: var(--font-roboto-condensed);
}
.menu-open.has-treeview::before {
    content: "";
    height: 99%;
    opacity: 1;
    width: 2px;
    position: absolute;
    left: 10px;
    border-radius: 15px;
    background: rgba(255, 255, 255, 0.3) !important;
    top: 0;
}
.sidebar-collapse .menu-open.has-treeview::before {
    left: 31px;
}
.sidebar-collapse aside:hover .menu-open.has-treeview::before {
    left: 10px;
}
body.sidebar-collapse aside .nav-sidebar>.nav-item .nav-icon {
    padding-left: 10px;
}
body.sidebar-collapse aside:hover .nav-sidebar>.nav-item .nav-icon{
    padding-left: 0px;
}
body.sidebar-collapse aside .nav-sidebar .nav-item li a p {
    font-size: 6px;
}
body.sidebar-collapse aside .nav-sidebar .nav-item li a p{
    font-size: 14px;
}
body.sidebar-collapse aside .nav-sidebar>.nav-item> .nav-treeview{
    display:none;
}
body.sidebar-collapse aside:hover .nav-sidebar>.nav-item> .nav-treeview{
    display:block;
}
.nav-sidebar .nav-item>.nav-link p {
    padding-left: 9px;
    white-space: normal;
    font-weight: 300;
    font-size: 14px;
}
.nav-sidebar> .nav-item>.nav-link{
    margin-bottom: 0.2rem;
    font-size: 16px;
    color: #fff;
    border-bottom: 1px solid #29415c;
    border-radius: 0;
}
.nav-sidebar .nav-item:hover a, .nav-sidebar .nav-item.menu-open>.nav-link {
    border-radius: 10px;
}

.nav-sidebar .nav-item .nav-treeview>li>a {
    padding: 4px 12px;
}
body .sidebar .nav-sidebar> .nav-item>.nav-treeview {
    background: #1d2946;
    border-radius: 10px;
}
.nav-sidebar .nav-item .nav-treeview>li>a {
    padding: 5px 14px;
    border-bottom: 1px solid #3d5387;
    border-radius: 0;
}
body.sidebar-collapse.layout-fixed .wrapper .dsh_main aside .brand-link span {
    display: none;
}
body.sidebar-collapse.layout-fixed .wrapper .dsh_main aside .brand-link {
    overflow: visible;
    height: auto;
}
body.sidebar-collapse.layout-navbar-fixed.layout-fixed .wrapper .sidebar {
    margin-top: calc(7rem + 1px);
    height: calc(100vh - (7rem + 1px));
}
body.sidebar-collapse.layout-fixed .wrapper .dsh_main aside:hover .brand-link span {
    display: block;
}
body.sidebar-collapse.layout-fixed .wrapper .dsh_main aside:hover .nav-sidebar>li {
    margin-left: 0;
}
body.sidebar-collapse.layout-fixed .wrapper .dsh_main aside:hover .nav-sidebar>li> a p {
    width: 100%;
}
body aside .nav-sidebar>.nav-item .nav-icon {
    font-size: 17px !important;
}
/****************************
breadcrumds
****************************/
.content-prg {
    padding: 10px 3px 10px 3px;
}
.profile-completion span {
